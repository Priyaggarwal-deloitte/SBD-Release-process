	//This script is being used to update status and stage of the release project on the basis of build scope
	if(current.x_675641_sbd_relea_build_scope=='new build')
		{
			current.x_675641_sbd_relea_status = 'inDevelopment';
			current.x_675641_sbd_relea_stage='analysis';
		}
	else if(current.x_675641_sbd_relea_build_scope=='production controls assessment')
